# Base URLs for each provider
PROVIDER_1_BASE_URL=
PROVIDER_2_BASE_URL=
PROVIDER_3_BASE_URL=
PROVIDER_4_BASE_URL=
PROVIDER_5_BASE_URL=
PROVIDER_5_IMG_BASE_URL=
PROVIDER_6_BASE_URL=
PROVIDER_7_BASE_URL=
PROVIDER_8_BASE_URL=

# API Keys for each provider (Leave empty if not used)
PROVIDER_1_API_KEY="LET THIS BE EMPTY"
PROVIDER_2_API_KEY=
PROVIDER_3_API_KEY=
PROVIDER_4_API_KEY=
PROVIDER_5_API_KEY=
PROVIDER_6_API_KEY=
PROVIDER_7_API_KEY=
PROVIDER_8_API_KEY=

# Multiple API keys for Provider 7 rotation (JSON array)
PROVIDER_7_API_KEYS=[]
# Error codes that trigger rotation (JSON array, use ["*"] for all errors)
PROVIDER_7_ERROR_CODES=["*"]

# --- Provider 9 (Azure OpenAI) ---
AZURE_API_KEY=your_azure_openai_api_key
AZURE_ENDPOINT=your_azure_openai_endpoint
AZURE_API_VERSION=2024-02-01
AZURE_DEPLOYMENT_NAME=your_deployment_name

###############################################
# Firebase and Supabase Configuration
###############################################

# Firebase service account key path
FIREBASE_SERVICE_ACCOUNT_PATH=app/credentials/serviceAccountKey.json

# Supabase configuration
SUPABASE_URL=your-supabase-url
SUPABASE_KEY=your-supabase-key-here

###############################################
# Local System Configuration
###############################################

# Local API URL
LOCAL_API_URL=http://127.0.0.1:5000

# System secret for internal usage
SYSTEM_SECRET=

###############################################
# PostgreSQL Database Configuration
###############################################

POSTGRES_USER=postgres-user
POSTGRES_PASSWORD=your-installation-password
POSTGRES_DB=freeapi
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

###############################################
# Redis Configuration
###############################################

# Option 1: Local Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_USERNAME=

# Option 2: Upstash Redis Configuration
# Method A: Using REDIS_URL (Recommended)
# Note: Use rediss:// (with double 's') for SSL connection
# REDIS_URL=rediss://default:password@host.upstash.io:6379

# Method B: Using Individual Components
# Note: The system will automatically use SSL for Upstash domains
# REDIS_HOST=host.upstash.io
# REDIS_PORT=6379
# REDIS_PASSWORD=your-password-here
# REDIS_USERNAME=default

